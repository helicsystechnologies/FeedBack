- (IBAction)vgoodButton:(id)sender {
    UIAlertView *alert = [[UIAlertView alloc] initWithTitle:@"Thanks"
                                                    message:@"For Your Valuable Feedback."
                                                   delegate:self
                                          cancelButtonTitle:@"OK"
                                          otherButtonTitles:nil];
    [alert show];
}
- (IBAction)goodButton:(id)sender {
    
    UIAlertView *alert = [[UIAlertView alloc] initWithTitle:@"Thanks"
                                                    message:@"For Your Valuable Feedback."
                                                   delegate:self
                                          cancelButtonTitle:@"OK"
                                          otherButtonTitles:nil];
    [alert show];
}

- (IBAction)averageButton:(id)sender {
    
    UIAlertView *alert = [[UIAlertView alloc] initWithTitle:@"Thanks"
                                                    message:@"For Your Valuable Feedback."
                                                   delegate:self
                                          cancelButtonTitle:@"OK"
                                          otherButtonTitles:nil];
    [alert show];
}

- (IBAction)employeeLoginButton:(id)sender {
    
    
    UIAlertView *alert = [[UIAlertView alloc] initWithTitle:@"Welcome"
                                                    message:@"Enter Your Details."
                                                   delegate:self
                                          cancelButtonTitle:@"OK"
                                          otherButtonTitles:nil];
    [alert show];

    
    EmployeeViewController *Evc = [[EmployeeViewController alloc] initWithNibName:@"EmployeeViewController" bundle:nil];
    [self.navigationController pushViewController:Evc animated:YES];
    
    
}


@end
